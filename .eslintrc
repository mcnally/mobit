{
  "extends": "airbnb-base",
  "plugins": ["jest"],
  "env": {
    "jest": true
  },
  "rules": {
    "no-console": "off",
    "arrow-parens": "off",
    "no-plusplus": "off",
    "no-underscore-dangle": "off",
    "no-param-reassign": "off",
    "array-callback-return": "off",
    "import/prefer-default-export": "off"
  }
}
